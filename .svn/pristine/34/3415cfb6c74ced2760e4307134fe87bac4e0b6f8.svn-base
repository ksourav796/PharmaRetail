</div>
<div class="modal fade selectSupplier" tabindex="-1" id="selectSupplier" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true" ng-click="remove()">&times;</span></button>
                <h4 class="modal-title">Select Supplier</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12">
                        <label>Search: <input class="form-control input-sm"
                                              ng-keyup="$event.keyCode == 13 ? getSupplierListSearch(text) : null"
                                              ng-change-delay="300" ng-model="text"></label>
                        <div class="table-responsive">
                            <table class="table table-select-supplier table-striped table_costomize supplierList" id="">
                                <thead>
                                <tr>
                                    <th style="width: 33.3% " data-field="name">Supplier Name</th>
                                    <th style="width: 33.3% " data-field="desc">Email</th>
                                    <th style="width: 33.3% " data-field="desc">Contact</th>
                                </tr>
                                </thead>
                                <tbody id="parentSupplierTable">
                                <tr ng-repeat="supplierId in supplierList| filter:customerSearchText"
                                    ng-click="appendSupplier(supplierId)">
                                    <td style="width: 33.3% !important;word-break: break-word;">{{supplierId.customerName}}</td>
                                    <td style="width: 33.3% !important;word-break: break-word;">{{supplierId.customerEmail}}</td>
                                    <!--<td style="width: 33.3% !important;word-break: break-word;">{{supplierId.phoneNumber1}}</td>-->
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" ng-click="remove()" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<add-new-item-modal></add-new-item-modal>
<payment-modal></payment-modal>
<add-new-supplier-modal></add-new-supplier-modal>
</div>
