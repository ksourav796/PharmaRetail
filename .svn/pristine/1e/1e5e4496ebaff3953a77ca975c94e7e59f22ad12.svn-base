<div class="modal fade" tabindex="-1" id="add_new_item_modal" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel_1">
    <div class="modal-dialog modal-lg">
        <form class="form-horizontal" id="salesAddItem">
            <div class="modal-content" style="height: 563px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Add Item </h4>
                </div>
                <div class="modal-body checkout-body" style="height: 443px; overflow-y: auto;">
                    <div class="col-md-6">
                        <!-- Text input-->
                        <fieldset>
                            <div class="tab-content">
                                <div class="tab-pane fade in active"> <!--id="sub_step1"-->
                                    <div ng-show="showFirst">
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="ItemCode">Item Code/Bar Code
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <input id="ItemCode" name="ItemCode"
                                                       allowpattern="[a-z-0-9]"
                                                       type="text"maxlength="12" placeholder="Item Code"
                                                       ng-model="itemCodeText"
                                                       class="form-control input-md">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="ItemName">Item Name
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <input id="ItemName" name="ItemName"
                                                       allowpattern="[a-z]" maxlength="125"
                                                       type="text" placeholder="Item Name"
                                                       ng-model="itemNameText"
                                                       no-special-char=""
                                                       class="form-control input-md">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="itemDesc">Description</label>
                                            <div class="col-xs-6">
                                            <textarea name="itemDesc" cols="" rows="" id="itemDesc"
                                                      class="form-control input-md"
                                                      ng-model="itemDiscText"
                                                      maxlength="255"></textarea>
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="idItemCategory">Item Category
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <select name="idItemCategory.itemCategoryId" id="idItemCategory"
                                                        ng-model="itemCategoryId"
                                                        ng-options="itemCategory.itemCategoryName as itemCategory.itemCategoryName for itemCategory in itemCategoryDTOList"
                                                        class="form-control input-md">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1 no-pad">
                                                <a class="btn btn-default add_btn" ng-click="additemcategory()"
                                                   id="get-selections"><i
                                                        class="fa fa-plus fa-fw"></i></a>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="idItem">Item Type
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <select name="itemTypeId" id="itemTypeId" ng-model="itemTypeId"
                                                        ng-init="itemTypeId='product'" class="form-control input-md">
                                                    <option value="product">Product</option>
                                                    <option value="service">Service</option>
                                                </select>
                                                <div id="idItemTypeerror" style="color: red"></div>
                                            </div>
                                            <div class="col-xs-1">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="idItemUOM">Unit of Measurement
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <select name="idItemUOM.UnitOfMeasurementId" id="idItemUOM"
                                                        class="form-control input-md"
                                                        ng-model="unitOfMeasurementId"
                                                        ng-options="unitOfMeasurementId.unitOfMeasurementName as unitOfMeasurementId.unitOfMeasurementName for unitOfMeasurementId in itemUOMTypeDTOList">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1 no-pad">
                                                <a class="btn btn-default add_btn" ng-click="addNewUOM()"
                                                   id="get-selections"><i
                                                        class="fa fa-plus fa-fw"></i></a>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="msiccomid">HSN/SAC Code
                                                <span>*</span></label>
                                            <div class="col-xs-6">
                                                <select name="msiccomid.msiccomid" id="msiccomid"
                                                        ng-model="mscid"
                                                        ng-options="mscid.msiccode as mscid.msiccode for mscid in itemMSICDTOList"
                                                        class="form-control input-md">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                            <!--<div class="col-sm-1 col-md-1 col-lg-1 no-pad">-->
                                                <!--<a class="btn btn-default add_btn" ng-click="getHSNCodeListFromFile()"-->
                                                   <!--id="get-selections"><i-->
                                                        <!--class="fa fa-plus fa-fw"></i></a>-->
                                            <!--</div>-->
                                        </div>

                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="taxIPId">Input
                                                Tax<span>*</span></label>
                                            <div class="col-xs-6">
                                                <select id="taxIPId"
                                                        ng-model="taxIPId"
                                                        ng-options="taxIPId.taxName as taxIPId.taxCode for taxIPId in itemIPTaxDTOList"
                                                        class="form-control input-md">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="taxOPId">Output
                                                Tax<span>*</span></label>
                                            <div class="col-xs-6">
                                                <select id="taxOPId"
                                                        ng-model="taxOPId"
                                                        ng-options="taxOPId.taxName as taxOPId.taxCode for taxOPId in itemOPTaxDTOList"
                                                        class="form-control input-md">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label"
                                                   for="CESS">CESS(%) <span></span></label>
                                            <div class="col-xs-6">
                                                <input type="text" two-digits-dec limit-to="6"
                                                       id="CESS"
                                                       name="CESS"
                                                       class="form-control input-md"
                                                       ng-model="CESSText"
                                                       required/>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 control-label" for="itemBrandId">Brand Name</label>
                                            <div class="col-xs-6">
                                                <select name="brandName.brandId" id="itemBrandId"
                                                        ng-model="brandId"
                                                        ng-options="brandId.brandName as brandId.brandName for brandId in itemBrandDTOList"
                                                        class="form-control input-md">

                                                </select>
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1 no-pad">
                                                <a class="btn btn-default add_btn" ng-click="addNewBrand()"
                                                   id="get-selections"><i
                                                        class="fa fa-plus fa-fw"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-md-6">
                        <div ng-show="showSecond">
                        </div>
                        <div class="form-group">
                            <label class="col-xs-5 control-label" for="serializableStatus">Count
                                Type</label>
                            <div class="col-xs-6">
                                <select name="serializableStatus" id="serializableStatus" ng-model="inventoryMovementId"
                                        ng-init="inventoryMovementId='bulk'" class="form-control input-md">
                                    <option value="bulk">Bulk</option>
                                    <option value="serialize">Serialize</option>
                                    <option value="batch">Batch</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-5 control-label" for="itemStatus">Status</label>
                            <div class="col-xs-6">
                                <select name="itemStatus" id="itemStatus" ng-model="itemStatusText"
                                        ng-init="itemStatusText='Active'" class="form-control input-md">
                                    <option value="Active">Active</option>
                                    <option value="InActive">InActive</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-5 control-label" for="re-OrderLevel">Re-OrderLevel</label>
                            <div class="col-xs-6">
                                <input id="Re-Order Level" name="Re-OrderLevel"
                                       type="text" placeholder="Re-OrderLevel "
                                       ng-model="reOrderLevelText"
                                       num-with-space
                                       class="form-control input-md" maxlength="15">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-5 control-label"
                                   for="purchasePricing">Purchase Pricing <span></span></label>
                            <div class="col-xs-6">
                                <input type="text" limit-to="8" min="0"
                                       id="purchasePricing"
                                       name="purchasePricing"
                                       class="form-control input-md"
                                       ng-model="purchasePricingText"
                                       ng-pattern="/^[0-9]+(\.[0-9]{1,2})?$/" step="0.01" required/>
                            </div>
                            <input type="checkbox" ng-model="inTax" style="margin-top: 14px;" ng-init="inTax = inTax || false"/>

                        </div>
                        <div class="form-group">
                            <label class="col-xs-5 control-label"
                                   for="salesPricing">Sales Pricing <span></span></label>
                            <div class="col-xs-6">
                                <input type="text" limit-to="8" min="0"
                                       id="salesPricing"
                                       name="salesPricing"
                                       class="form-control input-md"
                                       ng-model="salesPricingText"
                                       ng-pattern="/^[0-9]+(\.[0-9]{1,2})?$/" step="0.01" required/>
                            </div>
                            <input type="checkbox" ng-model="outTax" style="margin-top: 14px;"
                                   ng-init="outTax = outTax || false"/>
                        </div>
                        <!--<div class="form-group">-->
                            <!--<label class="col-sm-5 col-xs-5 control-label">Certificate No<span></span></label>-->
                            <!--<div class="col-sm-6 col-xs-6">-->
                                <!--<input type="text" limit-to="10" min="0" num-with-space-->
                                       <!--id="certNo" name="certNo" placeholder="Certificate No"-->
                                       <!--class="form-control input-md"-->
                                       <!--ng-model="certNoText"-->
                                       <!--ng-pattern="/^[0-9]+(\.[0-9]{1,2})?$/" step="0.01" required/>-->
                            <!--</div>-->
                        <!--</div>-->

                        <div class="form-group">
                            <label class="col-xs-5 control-label"
                                   for="production">Production Item <span></span></label>
                            <div class="col-xs-6">
                                <input type="checkbox" ng-model="productionItem"
                                       ng-init="productionItem = productionItem || false"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-xs-5 control-label" for="">Attribute :</label>
                            <div class="col-xs-6 control-label"
                                 style="border: 1px solid #ccc; overflow: auto; max-height: 100px;">
                                <div style="width: 344px;">
                                    <div class="col-lg-4"
                                         style="display:inline; margin: 0px; padding: 0px 5px; text-align: left"
                                         ng-repeat="x in attributeConfiguratorDTOList">
                                        <input type="checkbox"
                                               id="{{x.attributeConfiguratorId}}"
                                               name="attributeConfiguratorConvertedName"
                                               ng-model="x.status"
                                               ng-change="selectModule(x);">
                                        {{x.attributeConfiguratorName}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-6 col-xs-offset-2">
                                <input type='file' accept='image/*' id="imageLoad" ng-model-instant
                                       onchange="angular.element(this).scope().imageUpload(event)"><br>
                                <img class="image-preview"
                                     style="width:200px;height:200px; margin-left: 39px;">
                            </div>
                        </div>
                        <!--<a class="btn btn-default back" href="javascript:void(0)"
                           ng-click="showFirstSecond('first')"
                           data-toggle="tab" data-step="2">{{moreOrLessText}}</a>-->
                        <!--ng-click="back_wizard()"-->
                    </div>
                </div>

                <!--<div class="form-group" id="cerificatenodiv" style="display: none">-->
                <!--<label class="col-xs-5 control-label" for="input Tax">Certificate No</label>-->
                <!--<div class="col-xs-6">-->
                <!--<input type="text" name="certificateno" maxlength="20" value="" id="certificateno"-->
                <!--class="form-control input-md" placeholder="Certificate No">-->
                <!--<div id="certificatenoerror" style="color: red"></div>-->
                <!--</div>-->
                <!--</div>-->


                <div class="modal-footer">
                    <!--<button type="submit" id="colButn" class="btn btn-default" data-toggle="modal"-->
                            <!--ng-model="cartStatus" data-target=".print-me" ng-click="changeColor()">{{addRemoveCart}}-->
                    <!--</button>-->
                    <button type="submit" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                            data-target=".print-me" ng-click="saveNewItem('add_new_item_modal');blankImgSrc()">Save
                    </button>
                    <button type="button" class="btn btn-default" ng-click="removeItemPopUpDetails();blankImgSrc()"
                            data-dismiss="modal">Close
                    </button>
                </div>
            </div>
        </form>
    </div>
</div>

<div class="modal fade addCustomer" tabindex="-1" id="add_new_ItemCategory_modal" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="modelName">Add Category</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form class="form-horizontal">
                            <fieldset>
                                <input type="hidden" ng-model="ItemObj.itemCategoryId"
                                       value="{{ItemObj.itemCategoryId}}"/>
                                <div>
                                    <label class="col-md-4 col-sm-4 control-label">Category Name</label>
                                    <div class="col-md-6 col-sm-6" style="position:relative;">
                                        <input id="CategoryName" name="CategoryName"
                                               allowpattern="[a-z]" alphanum-with-space
                                               type="text" placeholder="Category Name"
                                               ng-value="ItemObj.itemCategoryName"
                                               ng-model="CategoryNameText"
                                               class="form-control input-md">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 col-sm-4 control-label">Category Description</label>
                                    <div class="col-md-6 col-sm-6" style="position:relative;">
                                        <textarea name="CategoryDescription" id="CategoryDescription"
                                                  ng-value="ItemObj.itemCategoryDesc"
                                                  ng-model="CategoryDescriptionText"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 col-sm-4 control-label">Default Type</label>
                                    <div class="col-md-6 col-sm-6" style="position:relative;">
                                        <input id="defaultType" type="checkbox" ng-change="selectModule(defaultType);"
                                               ng-model="defaultType">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 col-sm-4 control-label" for="">Status</label>
                                    <div class="col-md-6 col-sm-6" >
                                        <select name="ctgryStatus" id="ctgryStatus" ng-model="ctgryStatusText" class="form-control input-md" required>
                                            <option value="Active">Active</option>
                                            <option value="InActive">InActive</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-default" id="submit" ng-disabled="isDisabled"
                                            ng-click="saveNewItemCategory()">Save
                                    </button>
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                </div>
                            </fieldset>

                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade addCustomer" tabindex="-1" id="add_new_brand_modal" role="dialog" data-backdrop="static"
