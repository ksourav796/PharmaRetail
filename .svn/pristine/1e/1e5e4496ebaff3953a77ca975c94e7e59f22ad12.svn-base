                                        <input id="UOMDescription" name="UOMDescription"
                                               allowpattern="[a-z]"
                                               type="text" placeholder="UOM Description"
                                               ng-model="UOMDescription"
                                               ng-value="uominvObj.unitOfMeasurementDescription"
