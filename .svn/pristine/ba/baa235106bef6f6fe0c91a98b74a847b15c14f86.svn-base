<div class="container-fluid">
    <div class="container cus_main_row">

        <div class="row">
            <h2 class="pos-header-new">
                <div>Student</div>
            </h2>
        </div>
        <div class="row inner_row_bg">
            <div class="col-md-12 col-sm-12 col-xs-12">

                <div class="panel">
                    <div class="panel-body">
                        <div class="panel-body" style="height: 65px;">
                            <div class="col-md-4 col-lg-2 col-sm-6 col-xs-6">
                                <form class="form-horizontal ng-pristine ng-valid">
                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <p class="input-group" style="display: block;">
                                                <input id="search_student" name="textinput" type="text"
                                                       placeholder="search StudentName" ng-model="SearchText"
                                                       class="form-control input-md"
                                                />
                                            </p>

                                        </div>
                                    </div>
                                </form>
                            </div>


                            <div class="col-md-9 col-lg-9 col-sm-9 col-xs-9">

                                <div class="form-group">
                                    <div class="col-sm-1">
                                        <button type="button" class="btn btn-primary hi_btn" ng-click="addNewStudent()">
                                            Add
                                        </button>
                                    </div>

                                    <div class="col-sm-3">
                                         <span class="custom-dropdown custom-dropdown--white">
                                           <select class="form-control input-md"
                                                   ng-model="gradeNameText"
                                                   ng-change="getStudentListBasedOnGrade(gradeNameText);getStudentList(SearchText,gradeNameText,studentText,checkboxStatusForStudent)"
                                                   ng-options="grade.gradeId as grade.gradeName for grade in gradeList">
                                                        <option value="">Select Grade</option>
                                           </select>
                                         </span>
                                    </div>
                                    <div class="col-sm-3">
                                         <span class="custom-dropdown custom-dropdown--white">
                                             <select class="form-control input-md"
                                                     ng-model="studentText"
                                                     ng-change="getStudentList(SearchText,gradeNameText,studentText)"
                                                     ng-options="student.studentId as student.studentName for student in studentListBasedOnGrade">
                                                        <option value="">Select Student</option>
                                           </select>
                                         </span>
                                    </div>

                                    <div class="col-sm-2">
                                        <a href="/bs/studentExportToExcel">
                                            <button type="button" class="btn btn-default hi_btn"
                                                    style="margin-bottom: 10px;background: #72319a;color: white">
                                                Excel
                                            </button>
                                        </a>
                                    </div>
                                    <div class="col-sm-1">

                                        <input type="checkbox" id="checkboxForStudent"
                                               ng-model="checkboxStatusForStudent"
                                               ng-init="checkboxStatusForStudent = checkboxStatusForStudent || false"
                                               ng-click="getStudentList(searchText,gradeNameText,studentText,checkboxStatusForStudent)"/>

                                    </div>
                                    <span style="align-content: right">InActive</span>

                                </div>


                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="fixed-table-container">
                        <div class="table_wrapfixed-table-container-inner">
                            <div class="header-background"></div>
                            <table data-toggle="table" data-cache="false" data-height="299"
                                   class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                                <thead>
                                <tr>
                                    <th data-field="cntryname">
                                        <div class="th-inner" align="center">Student Name</div>
                                    </th>
                                    <th data-field="edit">
                                        <div class="th-inner" align="center">Student Profile Id</div>
                                    </th>
                                    <th data-field="edit">
                                        <div class="th-inner" align="center">Grade</div>
                                    </th>
                                    <th data-field="edit">
                                        <div class="th-inner" align="center">Parent Contact No</div>
                                    </th>
                                    <th data-field="edit">
                                        <div class="th-inner" align="center">Status</div>
                                    </th>

                                    <th data-field="edit">
                                        <div class="th-inner" align="center">Actions</div>
                                    </th>
                                </tr>
                                </thead>
                                <tbody id="parentTable">
                                <tr ng-repeat="student in studentList|filter:SearchText">
                                    <td hidden>{{student.studentId}}</td>
                                    <td style="width: 20%!important;word-break: break-word">{{student.studentName}}</td>
                                    <td>{{student.studentProfileId}}</td>
                                    <td>{{student.gradeMaster.gradeName}}</td>
                                    <td>{{student.primaryContactNo}}</td>
                                    <td>{{student.studentStatus}}</td>

                                    <td>
                                        <!--<button type="button" class="btn btn-primary hi_btn"  ng-click="fee(feedetails)">Fee</button>-->
                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="ViewStudentFeeDetailsList(student)">View
                                        </button>
                                        <!--<div ng-if="student.studentStatus!='InActive'">-->
                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="studentFeeDetailsList(student)">Edit
                                        </button>
                                        <!--</div>-->
                                        <!--<div>-->
                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="deleteStudent(student)">Delete
                                        </button>
                                        <!--</div>-->
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

