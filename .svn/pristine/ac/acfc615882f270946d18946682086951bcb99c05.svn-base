package com.hyva.posretail.pos.posPojo;

import java.util.List;

public class EWallet {
    private String paymentType;
    private double totalEWBeforDiscount;
    private double totalEWDiscount;
    private double totalEWAmountTendered;
    private double totalEWAmountRefunded;
    private List<EwalletDetails> ewalletDEtailsList;

    public String getPaymentType() {
        return paymentType;
    }

    public void setPaymentType(String paymentType) {
        this.paymentType = paymentType;
    }

    public double getTotalEWBeforDiscount() {
        return totalEWBeforDiscount;
    }

    public void setTotalEWBeforDiscount(double totalEWBeforDiscount) {
        this.totalEWBeforDiscount = totalEWBeforDiscount;
    }

    public double getTotalEWDiscount() {
        return totalEWDiscount;
    }

    public void setTotalEWDiscount(double totalEWDiscount) {
        this.totalEWDiscount = totalEWDiscount;
    }

    public double getTotalEWAmountTendered() {
        return totalEWAmountTendered;
    }

    public void setTotalEWAmountTendered(double totalEWAmountTendered) {
        this.totalEWAmountTendered = totalEWAmountTendered;
    }

    public double getTotalEWAmountRefunded() {
        return totalEWAmountRefunded;
    }

    public void setTotalEWAmountRefunded(double totalEWAmountRefunded) {
        this.totalEWAmountRefunded = totalEWAmountRefunded;
    }

    public List<EwalletDetails> getEwalletDEtailsList() {
        return ewalletDEtailsList;
    }

    public void setEwalletDEtailsList(List<EwalletDetails> ewalletDEtailsList) {
        this.ewalletDEtailsList = ewalletDEtailsList;
    }
}
